{{#if blogs.length}}
  <h3> Current Projects: </h3>
  {{#each blogs as |blog|}}
    <div>
        <div>
            <h2>
                <a href="/blog/{{blog.id}}">{{blog.title}}</a>
            </h2>
            <p>{{format_date blog.created_date}}</p>
        </div>
    </div>

    {{/each}}
{{/if}}

